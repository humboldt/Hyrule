<h2>Edit your profile</h2>
  <%= form_for @user do |f| %>
    <%= f.email_field :email %>
    <%= f.fields_for :account do |account_f| %>
     <%= account_f.label      :name %>
     <%= account_f.text_field :name %>
     <%= account_f.label      :about %>
     <%= account_f.text_area :about %>
    <% end %>
   <%= f.submit 'Save' %>
  <% end %>
<%= link_to "Signout", destroy_user_session_path, method: :delete %>
<%= link_to 'Profile page', user_path(@user) %>
